<FindBugsFilter
        xmlns="https://github.com/spotbugs/filter/3.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">

    <Match>
        <Package name="~io\.oigres\.ecomm\.service\.limiter.*"/>
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>


    <Match>
        <Class name="io.oigres.ecomm.service.limiter.mps.reader.PendingQueue"/>
        <Bug pattern="CT_CONSTRUCTOR_THROW" />
    </Match>

    <Match>
        <Class name="io.oigres.ecomm.service.limiter.mps.reader.PendingQueue"/>
        <Bug pattern="EC_UNRELATED_TYPES_USING_POINTER_EQUALITY" />
    </Match>

    <Match>
        <Class name="io.oigres.ecomm.service.limiter.mps.reader.PendingQueue"/>
        <Bug pattern="MC_OVERRIDABLE_METHOD_CALL_IN_READ_OBJECT" />
    </Match>

    <Match>
        <Class name="io.oigres.ecomm.service.limiter.services.RequestServiceImpl"/>
        <Bug pattern="RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT" />
    </Match>

</FindBugsFilter>